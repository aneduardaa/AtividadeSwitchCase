#include<stdio.h>

main(){
 int ddd;
 printf("digite seu ddd\n");
 scanf("%d", &ddd);

 switch (ddd){
    case 61:
    printf("brasilia");
    break;
    case 71:
    printf("Salvador");
    break;
    case 11:
    printf("Sao Paulo");
    break;
    case 21:
    printf("Rio de Janeiro");
    break;
    case 32:
    printf("Juiz de Fora");
    break;
    case 19:
    printf("Campinas");
    case 27:
    printf("Campinas");
    break;
    case 31:
    printf("Belo Horizonte");
    break;
    default:
    printf("nenhuma das opcoes acima");

 }


